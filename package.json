{
  "name": "praxthon-project",
  "version": "1.0.0",
  "description": "This is a blockchain-based app built on Hyperledger Fabric along with react and nodejs",
  "license": "ISC",
  "author": "Daniel Valderrama",
  "type": "commonjs",
  "main": "server.js",
  "scripts": {
    "start:fabric": "./scripts/startFabric.sh",
    "deploy:chaincode": "./scripts/deployChaincode.sh",
    "register:admin": "node server/registerUser.js",
    "import:admin-user": "node server/importFabricAdmin.js",
    "dev": "node server.js",
    "build": "NODE_ENV=production webpack --mode production",
    "start": "NODE_ENV=production node server.js",
    "dev-with-react": "concurrently \"npm run dev\" \"webpack serve --mode development --config webpack.config.js --port 3001\""
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.2",
    "@mui/material": "^7.3.2",
    "axios": "^1.11.0",
    "body-parser": "^2.2.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "fabric-ca-client": "^2.2.20",
    "fabric-client": "^1.4.20",
    "fabric-network": "^2.2.20",
    "path": "^0.12.7",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "babel-loader": "^10.0.0",
    "concurrently": "^9.2.1",
    "css-loader": "^7.1.2",
    "html-webpack-plugin": "^5.6.4",
    "nodemon": "^3.1.10",
    "style-loader": "^4.0.0",
    "webpack": "^5.101.3",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  }
}
